    },
    {
      "name": "min",
      "type": "Any",
      "description": ["Minimum value (use \"\" if string)."],
      "optional": false
    },
    {
      "name": "max",
      "type": "Any",
      "description": ["Maximum value (use\"\" if string)."],
      "optional": false
    },
    {
      "name": "type",
      "type": "string",
      "description": ["Type (int/float/string)."],
      "optional": false
    }
