{
  "sourceFile": "surf\\_util",
  "usage": "credits(<lines>)",
  "description": ["Show scrolling credits at the top of players' screens."],
  "params": [
    {
      "name": "lines",
      "type": "Array",
      "description": ["Text to display in the credits."],
      "optional": false
    }
  ],
  "example": [
    "creditsLines = [];",
    "creditsLines[creditsLines.size] = \"--- ^3Surf Space^7 ---\";",
    "creditsLines[creditsLines.size] = \"--- ^3Created by ^2atrX^7 ---\";",
    "credits(creditsLines);"
  ]
}
